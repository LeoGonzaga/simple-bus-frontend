{
  "compilerOptions": {
    "target": "ES6",
    "lib": ["DOM", "ES2015"],
    "types": ["@testing-library/jest-dom"],
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./",
    "paths": {
      "@src/*": ["./src/*"]
    }
  }
}
